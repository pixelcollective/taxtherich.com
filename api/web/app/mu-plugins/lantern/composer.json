{
    "name": "pixelcollective/lantern",
    "type": "mu-plugin",
    "description": "Laravel Lumen Framework for a better WordPress",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "bedrock",
        "wordpress"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "htmlburger/carbon-fields": "^3.0",
        "laravel/lumen-framework": "5.8.*",
        "league/flysystem-aws-s3-v3": "^1.0",
        "silk/silk": "^0.12.1",
        "soberwp/intervention": "^1.2",
        "unicodeveloper/laravel-emoji": "dev-master",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "laravel/tinker": "^1.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Lantern\\": "lantern/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
